<h1 class="todo_list_title">Sharing To-Do List</h1>
<br></br>
<%= form_with(model: @todo_list, local: true) do |form| %>
    <div class="forms">
        <%= form.label :user %>
        <%= form.text_field :user %>

        <div class="actions">
        <%= form.submit %>
        </div>
    </div>

    
<% end %>
<div class="links">
    <%= link_to todo_lists_path do%>
        <i class="fa fa-arrow-left fa-2x"></i>
    <% end %>
</div>